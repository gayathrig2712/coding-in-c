#include<stdio.h>
#include<stdlib.h>
int main(void)
{
    int term;
    
    int n1=0,n2=1,n3,i;
    int *ptr;
    ptr=&term;
    printf("terms:");
    scanf("%d",&term);
    printf("%d\n%d\n",n1,n2);
    for(i=2;i<*ptr;i++)
    {
      n3=n1+n2;
      printf("%d\n",n3);
      n1=n2;
      n2=n3;
      
      }
    
    
}
