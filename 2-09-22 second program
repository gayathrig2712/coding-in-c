#include <stdio.h>
int max_of_four(int a,int b,int c,int d);
int main()
{
 int a,b,c,d,max;
 scanf("%d\n%d\n%d\n%d\n",&a,&b,&c,&d);
 max=max_of_four(a,b,c,d);
printf("%d",max);
}
int max_of_four(int a,int b,int c,int d)
{
    int max;
    if((a>b)&&(a>c)&&(a>d))
    {
       max=a;
    }
    else if((b>a)&&(b>c)&&(b>d))
    {
      max=b;
    }
    else if((c>a)&&(c>b)&&(c>d))
    {
       max=c;
    }
    else
    {
       max=d;
    }
    return max;
}
